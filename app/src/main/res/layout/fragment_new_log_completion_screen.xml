<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.logger.NewLogCompletionScreen">

        <ImageView
            android:id="@+id/doneCheck"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="100dp"
            android:src="@drawable/avd_done" />

        <TextView
            android:id="@+id/LoggedText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="220dp"
            android:text="Logged"
            android:visibility="invisible"
            android:textColor="#0f9"
            android:textSize="36sp"
            android:textStyle="normal" />

        <Button
            android:id="@+id/see_resources_upon_complete_log"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:visibility="invisible"
            android:layout_marginTop="420dp"
            android:text="See My Resources" />
    </FrameLayout>

</layout>
